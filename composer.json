{
    "name": "basecodeoy/support",
    "description": "A collection of helper functions and classes.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "BaseCode Oy",
        "laravel",
        "intl"
    ],
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/basecodeoy/support",
    "support": {
        "issues": "https://github.com/basecodeoy/support/issues",
        "source": "https://github.com/basecodeoy/support",
        "security": "https://github.com/basecodeoy/support/blob/main/.github/SECURITY.md"
    },
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-mbstring": "*",
        "barryvdh/laravel-ide-helper": "^3.2.2",
        "brick/money": "^0.8.1",
        "illuminate/contracts": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/support": "^11.0",
        "spatie/laravel-data": "^4.11.1",
        "spatie/laravel-model-info": "^2.0.3"
    },
    "require-dev": {
        "basecodeoy/standards": "^1.1.0"
    },
    "autoload": {
        "psr-4": {
            "BaseCodeOy\\Support\\": "src"
        },
        "files": [
            "src/Functions/array.php",
            "src/Functions/datetime.php",
            "src/Functions/float.php",
            "src/Functions/integer.php",
            "src/Functions/money.php",
            "src/Functions/string.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        },
        "audit": {
            "abandoned": "report"
        },
        "preferred-install": "dist",
        "sort-packages": true
    }
}
